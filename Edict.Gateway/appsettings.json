{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "api": {
        "ClusterId": "api",
        "Match": {
          "Path": "/api/{**catch-all}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/api"
          }
        ]
      },
      "scalar": {
        "ClusterId": "scalar",
        "Match": {
          "Path": "/scalar/{**catch-all}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/scalar"
          }
        ]
      },
      "app": {
        "ClusterId": "app",
        "Match": {
          "Path": "/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "api": {
        "Destinations": {
          "d1": {
            "Address": "http://api"
          }
        }
      },
      "scalar": {
        "Destinations": {
          "d1": {
            "Address": "http://scalar"
          }
        }
      },
      "app": {
        "Destinations": {
          "d1": {
            "Address": "http://app:3000"
          }
        }
      }
    }
  }
}
